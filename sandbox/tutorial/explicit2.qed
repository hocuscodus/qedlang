String[] getPartner(String name) {
  String[] partner = []

  if (name == "John")
    partner = ["Jane"]

  return partner
}

void printPartner(String name) {
  String[] partner = getPartner(name)

  if (partner.size())
    println(name + "'s partner: " + partner[0])
  else
    println(name + " has no partner")
}

printPartner("John")
printPartner("Andrew")
